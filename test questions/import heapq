import heapq

def findKthLargest(nums, k):
    return heapq.nlargest(k, nums)[-1]

# Example 1
nums1 = [3, 2, 1, 5, 6, 4]
k1 = 2
output1 = findKthLargest(nums1, k1)
print(output1)  # Output: 5

# Example 2
nums2 = [3, 2, 3, 1, 2, 4, 5, 5, 6]
k2 = 4
output2 = findKthLargest(nums2, k2)
print(output2)  # Output: 4
